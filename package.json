{
  "name": "el-danes",
  "description": "Sapper website for El DANES",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run dsstore:delete && sapper dev",
    "build": "sapper build --legacy",
    "export": "npm run dsstore:delete && sapper export --legacy --entry '/ /en /en/cases /en/contact /da/cases /da/contact /es /es/cases /es/contact /404'",
    "postexport": "mv __sapper__/export/404/index.html __sapper__/export/404.html",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "fix": "npx eslint --fix \"{,!(node_modules|public)/**/}*.{js,svelte}\"",
    "dsstore:delete": "find . -name \".DS_Store\" -delete",
    "format": "npx prettier --write \"{,!(node_modules|public)/**/}*.{js,svelte}\""
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "^0.5.2",
    "sirv": "^1.0.6"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "maintained node versions"
  ],
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-yaml": "^2.1.1",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-svelte3": "^2.7.3",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0",
    "prettier": "^2.1.1",
    "prettier-plugin-svelte": "^1.2.1",
    "rollup": "^2.26.11",
    "rollup-plugin-inline-svg": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.6",
    "svelte": "^3.25.0",
    "svelte-preprocess": "^4.2.1",
    "svelte-prism": "^1.1.3"
  }
}
