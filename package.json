{
  "name": "el-danes",
  "description": "Website for El DANÃ‰S SOLUTIONS",
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite dev\" \"npm:check:watch\" \"npm:typesafe-i18n\"",
    "build": "vite build",
    "postbuildXXXX": "mv build/404/index.html build/404.html",
    "start": "svelte-kit start",
    "preview": "vite preview --port 3001",
    "lint": "prettier --check . && eslint --ignore-path .gitignore .",
    "update": "npx npm-check-updates -i",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:lint": "eslint --max-warnings=0 --ignore-path .eslintignore",
    "check:prettier": "prettier -c",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "typesafe-i18n": "typesafe-i18n"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "maintained node versions"
  ],
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.3",
    "@sveltejs/kit": "^2.5.21",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@tsconfig/svelte": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "cssnano": "^7.0.5",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-svelte": "^2.43.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "postcss": "^8.4.41",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "stylelint": "^16.8.1",
    "stylelint-config-prettier": "^9.0.5",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.5",
    "svelte-preprocess": "^6.0.2",
    "tslib": "^2.6.3",
    "typesafe-i18n": "^5.26.2",
    "typescript": "^5.5.4",
    "vite": "^5.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,svelte}": [
      "npm run check --",
      "npm run check:lint --",
      "npm run check:prettier -- --write"
    ]
  }
}
